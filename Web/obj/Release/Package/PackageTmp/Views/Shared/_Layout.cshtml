@using Web.AppCode;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A travel agency for students, Visit your university exam room with us.">
    <meta name="author" content="">
    <title>Admission Travel | @ViewBag.Title</title>

    <!-- Favicons Icon -->
    @*<link rel="icon" href="@Url.Content("~/images/favicon.ico")" type="image/x-icon" />*@

    <!-- Mobile Specific -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- CSS Style -->
    <link href="@Url.Content("~/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/font-awesome.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/owl.carousel.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/owl.theme.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/jquery.mobile-menu.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/admin-assets/css/bootstrap-switch.css")" rel="stylesheet">
    @*<link href="@Url.Content("~/kendo/css/kendo.common-material.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/kendo/css/kendo.material.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/kendo/css/kendo.material.mobile.min.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/kendo/css/kendo.common.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/kendo/css/kendo.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/kendo/css/kendo.mobile.all.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/kendo/css/kendo.common-material.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/revslider.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/blogmate.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/cloudzoom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/error.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/app.custom.css")" rel="stylesheet" type="text/css" />

    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:700,600,800,400' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Poppins:400,300,500,600,700' rel='stylesheet' type='text/css'>
    <script src="@Url.Content("~/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/js/owl.carousel.min.js")"></script>
    <script src="@Url.Content("~/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/js/revslider.js")"></script>
    <script src="@Url.Content("~/admin-assets/js/bootstrap-switch.js")"></script>
    <script src="@Url.Content("~/admin-assets/kendo/js/kendo.all.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/admin-assets/kendo/js/kendo.aspnetmvc.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/admin-assets/kendo/js/jszip.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/js/common.js")"></script>
    <script type="text/javascript">
        var pathURL = window.location.pathname; // Returns path only (/path/example.html)
        var pageURL = window.location.href;     // Returns full URL (https://example.com/path/example.html)
        var domainURL = window.location.origin;   // Returns base URL (https://example.com)
        var appUserName = "@LoggedInUserInfoFromCookie.UserDisplayNameInCookie";
    </script>
    <script src="@Url.Content("~/js/app.js")"></script>
    <script src="@Url.Content("~/js/validator/jquery.validate.js")"></script>

</head>

<body class="shopping-cart-page">
    <div id="page">
        <div class="notification-message" id="dummy_ajax_message"><div class="dummy_ntm_text" style="color: green; font-size: 16px;"></div></div>

        <!-- Header -->
        <header>
            <div class="header-container">

                <div class="container">
                    <div class="row">
                        <div class="col-xs-12 col-sm-8 col-md-6 col-lg-6 pull-right">
                            <div class="toplinks">
                                <div class="links">
                                    <div class="demo"><a title="Buy Ticket" href="@Url.Action("student-service", "agency")">Buy Ticket</a> </div>
                                    <div class="demo"><a title="Our News" href="@Url.Action("Index", "blog")">News</a> </div>
                                   
                                     @if (LoggedInUserInfoFromCookie.UserDisplayNameInCookie != null && LoggedInUserInfoFromCookie.UserDisplayNameInCookie != "")
                                     {
                                        <div class="demo"><a title="Blog" href="@Url.Action("dashboard", "admin")">Dashboard</a> </div>
                                        <div class="demo"><a title="Blog" href="@Url.Action("Logout", "account")">Log Out</a> </div>

                                     }
                                     else
                                     {
                                        <div class="demo"><a title="Blog" href="@Url.Action("login", "account")">Agent LogIn</a> </div>
                                     }
                                </div>
                            </div>
                            <!-- End Header Top Links -->
                        </div>
                    </div>
                </div>
            </div>

            @Html.Action("_LoadMainMenu", "home")
        </header>
        <!-- end header -->
        <!-- Main Container -->
        @RenderBody()
        <!-- Main Container End -->
        <!-- Footer -->
        <footer class="footer">
            @Html.Action("_LoadWebSiteFooter", "home")

        </footer>
    </div>
    <!-- End Footer -->
    <!-- JavaScript -->
    @*@Scripts.Render("~/bundles/jquery")*@

</body>
</html>
